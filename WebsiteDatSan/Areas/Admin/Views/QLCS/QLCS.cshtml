
@{
    ViewBag.Title = "QLCS";
}

@*@model List<WebsiteDatSan.Models.ApplicationUser>*@

<h1>Danh sách Chủ sân</h1>

@if (Model.Count > 0)
{
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Tên đăng nhập</th>
                <th>Email</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    <td>@user.Id</td>
                    <td>@user.UserName</td>
                    <td>@user.Email</td>
                </tr>
            }
        </tbody>
    </table>
}
else
{
    <p>Không có Chủ sân nào.</p>
}
