@model IEnumerable<WebsiteDatSan.Models.San>

@{
    ViewBag.Title = "ShowSanChuSan";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>ShowSanChuSan</h2>

<table class="table">
    <tr>
@*        <th>
            @Html.DisplayNameFor(model => model.LoaiSan.TenLoaiSan)
        </th>*@
        <th>
            Tên sân
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DiaChi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GiaTien)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrangThai)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdUser)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*        <td>
            @Html.DisplayFor(modelItem => item.LoaiSan.TenLoaiSan)
        </td>*@
        <td>
            @Html.ActionLink(item.TenSan, "ShowGioDats", new { id = item.MaSan })
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DiaChi)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GiaTien)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrangThai)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IdUser)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id = item.MaSan }) |
            @Html.ActionLink("Back to List", "Index")
        </td>
    </tr>
}

</table>
